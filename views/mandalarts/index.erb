<h1>マンダラート機能</h1>
<p>自分だけのマンダラートを作成しよう</p>

<% @workspaces.each do |workspace| %>
	<p class="p-size"><%= workspace.name %></p>
  <a href="/edit/mandalart/<%= workspace.id %>"><i class="fas fa-edit fa-2x"></i></a>
  <form action="/delete/mandalart/<%= workspace.id %>" method="post">
	  <input type="submit" value="DELETE">
  </form>
<% end %>

<a href="/index"><i class="fas fa-undo-alt fa-4x"></i></a><br>
<a href="/workspace_title"><i class="fas fa-plus-circle fa-4x"></i>
